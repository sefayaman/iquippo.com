'use strict';

var mongoose = require('mongoose'),
    Schema = mongoose.Schema;

//var timestamp=require('unix-timestamp');

var LeadMasterDataSchema = new Schema({
	leadId:String,
	customerName: String,
	mobile:String,
	assetName:String,
	offerDetail:String,
	status: String,
	
});

module.exports = mongoose.model('LeadMasterData', LeadMasterDataSchema);